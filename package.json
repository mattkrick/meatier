{
  "name": "meatier",
  "version": "0.9.0",
  "description": "Like meteor, but meatier",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattkrick/meatier.git"
  },
  "bugs": {
    "url": "https://github.com/mattkrick/meatier/issues"
  },
  "main": "index.js",
  "scripts": {
    "postinstall":  "rimraf build && concurrently \"npm run build:db\" \"npm run build:client\" \"npm run build:server\"",
    "reset":        "npm run postinstall",
    "dev":          "NODE_ENV=development node ./src/server/server.babel.js",
    "prod":         "NODE_ENV=production node ./src/server/server.babel.js",
    "build":        "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\"",
    "restart":      "npm run build && npm run prod",
    "restart:dev":  "npm run build && npm run dev",
    "start":        "npm run prod",
    "start:dev":    "npm run dev",
    "build:client": "NODE_ENV=production webpack -p --config ./webpack/prod.babel.js",
    "build:server": "NODE_ENV=production webpack -p --config ./webpack/server.babel.js",
    "build:db":     "node ./src/server/database/setupDB.babel.js",
    "test":         "NODE_ENV=testing ava ./src/**/__tests___/**/*.js --verbose",
    "lint":         "xo src/**/*.js --plugin=react --env node --env browser --esnext --space --fix",
    "profile":      "NODE_ENV=production webpack --config ./webpack/prod.babel.js --profile --json > stats.json",
    "help":         "npm run"
  },
  "author": "Matt Krick <matt.krick@gmail.com>",
  "contributors": [
    {
      "name": "Bartek Kus",
      "email": "bartekus@gmail.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "3.3.0",
    "ava": "0.11.0",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.2",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-static-fs": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.5.2",
    "concurrently": "2.0.0",
    "css-loader": "0.23.1",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fake-style-loader": "1.0.0",
    "file-loader": "0.8.5",
