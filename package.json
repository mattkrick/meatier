{
  "name": "meatier",
  "version": "0.10.0",
  "description": "Like meteor, but meatier",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattkrick/meatier.git"
  },
  "bugs": {
    "url": "https://github.com/mattkrick/meatier/issues"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node ./src/server/server.babel.js",
    "prod": "NODE_ENV=production node ./src/server/server.babel.js",
    "build": "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\"",
    "bs": "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\" \"npm run prod\"",
    "quickstart": "rimraf build && concurrently \"npm run build:db\" \"npm run build:client\" \"npm run build:server\" \"npm run prod\"",
    "build:client": "NODE_ENV=production webpack --config ./webpack/prod.babel.js",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:db": "node ./src/server/database/setupDB.babel.js",
    "test": "NODE_ENV=testing ava ./src/**/__tests___/**/*.js --verbose",
    "lint": "xo --fix src/**/*.js",
    "profile": "NODE_ENV=production webpack --config ./webpack/prod.babel.js --profile --json > stats.json"
  },
  "author": "Matt Krick <matt.krick@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "3.3.0",
    "ava": "0.12.0",
    "babel-cli": "6.6.4",
    "babel-core": "6.6.4",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-static-fs": "1.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.6.0",
    "concurrently": "2.0.0",
    "css-loader": "0.23.1",
    "eslint": "2.2.0",
    "eslint-config-xo-react": "0.5.0",
    "eslint-plugin-react": "4.1.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fake-style-loader": "1.0.0",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "node-sass": "^3.4.2",
    "null-loader": "0.1.1",
    "postcss-loader": "0.8.1",
    "postcss-modules-values": "1.1.1",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "0.14.7",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.2",
    "redbox-react": "1.2.2",
    "rimraf": "2.5.2",
    "sass-loader": "^3.1.2",
    "stats-webpack-plugin": "0.3.1",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.4",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.9.1",
    "xo": "0.12.1"
  },
  "dependencies": {
    "babel-polyfill": "6.6.1",
    "babel-runtime": "6.6.1",
    "bcrypt": "0.8.5",
    "body-parser": "1.15.0",
    "compression": "1.6.1",
    "cors": "2.7.1",
    "deep-equal": "1.0.1",
    "es6-promisify": "3.0.0",
    "express": "4.13.4",
    "express-jwt": "3.3.0",
    "googleapis": "2.1.7",
    "graphiql": "0.5.0",
    "graphql": "0.4.18",
    "immutable": "3.7.6",
    "isomorphic-fetch": "2.2.1",
    "joi": "8.0.4",
    "jsonwebtoken": "5.7.0",
    "jwt-decode": "1.5.1",
    "node-env-flag": "0.1.0",
    "node-uuid": "1.4.7",
    "normalize.css": "^3.0.3",
    "piping": "0.3.0",
    "query-string": "3.0.1",
    "react": "0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dnd": "2.1.2",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "0.14.7",
    "react-dom-stream": "0.5.0",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-toolbox": "^0.14.2",
    "redux": "3.3.1",
    "redux-form": "4.2.0",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "2.6.1",
    "redux-optimistic-ui": "0.3.2",
    "redux-promise": "0.5.1",
    "redux-simple-router": "2.0.4",
    "redux-socket-cluster": "0.5.1",
    "redux-thunk": "1.0.3",
    "reselect": "2.0.3",
    "rethinkdbdash": "2.2.18",
    "socketcluster": "4.3.2",
    "socketcluster-client": "4.3.7",
    "url": "0.11.0"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "env": [
      "browser",
      "node",
      "webpack"
    ],
    "extends": "xo-react",
    "globals": [
      "__CLIENT__",
      "__PRODUCTION__"
    ],
    "rules": {
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-indent-props": [
        2,
        2
      ]
    }
  }
}
