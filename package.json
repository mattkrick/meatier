{
  "name": "meatier",
  "version": "0.10",
  "description": "Like meteor, but meatier",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattkrick/meatier.git"
  },
  "bugs": {
    "url": "https://github.com/mattkrick/meatier/issues"
  },
  "main": "index.js",
  "scripts": {
    "postinstall":  "rimraf build && concurrent \"npm run build:db\" \"npm run build:client\" \"npm run build:server\"",
    "reset":        "npm run postinstall",
    "dev":          "NODE_ENV=development node ./src/server/server.babel.js",
    "prod":         "NODE_ENV=production node ./src/server/server.babel.js",
    "build":        "rimraf build && concurrent \"npm run build:client\" \"npm run build:server\"",
    "restart":      "npm run build && npm run prod",
    "restart:dev":  "npm run build && npm run dev",
    "start":        "npm run prod",
    "start:dev":    "npm run dev",
    "build:client": "NODE_ENV=production webpack -p --config ./webpack/prod.babel.js",
    "build:server": "NODE_ENV=production webpack -p --config ./webpack/server.babel.js",
    "build:db":     "node ./src/server/database/setupDB.babel.js",
    "test":         "NODE_ENV=testing ava ./src/**/__tests___/**/*.js --verbose",
    "lint":         "xo --fix src/**/*.js",
    "profile":      "NODE_ENV=production webpack --config ./webpack/prod.babel.js --profile --json > stats.json",
    "help":         "npm run"
  },
  "author": "Matt Krick <matt.krick@gmail.com>",
  "contributors": [
    {
      "name": "Bartek Kus",
      "email": "bartekus@gmail.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "3.2.0",
    "ava": "0.10.0",
    "babel-cli": "6.4.0",
    "babel-core": "6.4.0",
    "babel-loader": "6.2.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.4.3",
    "concurrently": "1.0.0",
    "css-loader": "0.23.1",
    "eslint": "^2.1.0",
    "eslint-config-xo-react": "^0.4.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fake-style-loader": "1.0.0",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "null-loader": "0.1.1",
    "postcss-loader": "0.8.0",
    "postcss-modules-values": "1.1.1",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "0.14.6",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "rimraf": "2.5.0",
    "stats-webpack-plugin": "0.3.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "2.0.2-beta",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0",
    "xo": "0.12.1"
  },
  "dependencies": {
    "babel-polyfill": "6.3.14",
    "babel-runtime": "6.3.19",
    "bcrypt": "0.8.5",
    "body-parser": "1.14.2",
    "compression": "1.6.0",
    "cors": "2.7.1",
    "deep-equal": "1.0.1",
    "es6-promisify": "3.0.0",
    "express": "4.13.3",
    "express-jwt": "3.3.0",
    "googleapis": "2.1.7",
    "graphiql": "0.4.5",
    "graphql": "^0.4.18",
    "immutable": "3.7.6",
    "isomorphic-fetch": "2.2.1",
    "joi": "7.2.1",
    "jsonwebtoken": "5.5.4",
    "jwt-decode": "1.5.0",
    "material-ui": "0.14.2",
    "node-uuid": "1.4.7",
    "piping": "0.3.0",
    "query-string": "3.0.0",
    "react": "^0.14.7",
    "react-dnd": "2.0.2",
    "react-dnd-html5-backend": "2.0.2",
    "react-dom": "0.14.6",
    "react-dom-stream": "0.4.1",
    "react-redux": "^4.4.0",
    "react-router": "2.0.0-rc5",
    "react-tap-event-plugin": "0.2.1",
    "redux": "^3.3.1",
    "redux-form": "4.1.3",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "2.3.2",
    "redux-optimistic-ui": "0.3.1",
    "redux-promise": "0.5.0",
    "redux-simple-router": "2.0.3",
    "redux-socket-cluster": "0.5.1",
    "redux-thunk": "1.0.3",
    "reselect": "2.0.2",
    "rethinkdbdash": "2.2.17",
    "socketcluster": "4.1.0",
    "socketcluster-client": "4.1.2",
    "url": "0.11.0"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "env": [
      "browser",
      "node",
      "webpack"
    ],
    "extends": "xo-react",
    "globals": [
      "__CLIENT__",
      "__PRODUCTION__"
    ],
    "rules": {
      "react/jsx-indent": [2, 2],
      "react/jsx-indent-props": [2, 2]
    }
  }
}
