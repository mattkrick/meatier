{
  "name": "meatier",
  "version": "0.10.0",
  "description": "Like meteor, but meatier",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattkrick/meatier.git"
  },
  "bugs": {
    "url": "https://github.com/mattkrick/meatier/issues"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node ./src/server/server.babel.js",
    "prod": "NODE_ENV=production node ./src/server/server.babel.js",
    "build": "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\"",
    "bs": "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\" \"npm run prod\"",
    "quickstart": "rimraf build && concurrently \"npm run build:db\" \"npm run build:client\" \"npm run build:server\" \"npm run prod\"",
    "build:client": "NODE_ENV=production webpack --config ./webpack/prod.babel.js",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:db": "node ./src/server/database/setupDB.babel.js",
    "update:db": "node ./src/server/database/setupDB.babel.js true",
    "test": "NODE_ENV=testing ava --verbose",
    "lint": "xo --fix 'src/**/*.js' 'tests/*.js' 'webpack/*.js'",
    "profile": "NODE_ENV=production webpack --config ./webpack/prod.babel.js --profile --json > stats.json"
  },
  "author": "Matt Krick <matt.krick@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "3.4.0",
    "ava": "0.16.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-static-fs": "1.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.14.0",
    "concurrently": "2.2.0",
    "css-loader": "0.25.0",
    "eslint": "3.5.0",
    "eslint-config-xo-meatier": "0.2.2",
    "eslint-config-xo-react": "0.9.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fake-style-loader": "1.0.1",
    "file-loader": "0.9.0",
    "happypack": "2.2.1",
    "json-loader": "0.5.4",
    "null-loader": "0.1.1",
    "postcss-loader": "0.13.0",
    "postcss-modules-values": "1.2.2",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.3.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.3.1",
    "rimraf": "2.5.4",
    "stats-webpack-plugin": "0.4.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.4",
    "webpack-dev-middleware": "1.8.1",
    "webpack-hot-middleware": "2.12.2",
    "xo": "0.16.0"
  },
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "babel-runtime": "6.11.6",
    "bcrypt": "0.8.7",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "deep-equal": "1.0.1",
    "dotenv": "2.0.0",
    "dotenv-expand": "0.1.0",
    "es6-promisify": "4.1.0",
    "express": "4.14.0",
    "express-jwt": "3.4.0",
    "googleapis": "5.2.1",
    "graphiql": "0.7.8",
    "graphql": "0.7.0",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "joi": "8.1.0",
    "jsonwebtoken": "6.2.0",
    "jwt-decode": "2.0.1",
    "material-ui": "0.15.4",
    "node-env-flag": "0.1.0",
    "node-uuid": "1.4.7",
    "piping": "1.0.0-rc.4",
    "react": "15.3.2",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "15.3.2",
    "react-dom-stream": "0.5.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux-form": "5.2.3",
    "redux": "3.6.0",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "2.6.1",
    "redux-optimistic-ui": "0.4.1",
    "redux-promise": "0.5.3",
    "redux-socket-cluster": "0.5.1",
    "redux-thunk": "2.1.0",
    "socketcluster": "4.6.1",
    "socketcluster-client": "4.3.17",
    "rethinkdbdash": "2.3.23"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "xo": {
    "extends": "xo-meatier",
    "rules": {
      "import/imports-first": 1,
      "import/newline-after-import": 0,
      "import/no-extraneous-dependencies": 1,
      "import/order": 0,
      "no-mixed-operators": 1,
      "react/jsx-filename-extension": 0,
      "react/require-optimization": 1,
      "xo/no-abusive-eslint-disable": 1
    }
  }
}
